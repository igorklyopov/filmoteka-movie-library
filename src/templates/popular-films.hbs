<h2 class="popular-movies-title">Popular movies</h2>
<ul class="list popular-btns-list">
    <li class="popular-btns-item">
        <button type="button" class="popular-btn day">Day</button>
    </li>
    <li>
        <button type="button" class="popular-btn week">Week</button>
    </li class="popular-btns-item">
</ul>

<ul class="list movies-list">
    {{#each this}}
    <li class="card-library">
        <img class="img-card-library" src="https://image.tmdb.org/t/p/original{{poster_path}}" alt="" data-src="" width="274" height="398"/>
        <div class="movie-information">
            <h3 class="movie-name">{{title}}</h3>
            <div class="generes-rating">
                <p class="genres">{{genre_ids}} | {{release_date}}</p>
             </div>
        </div>
    </li>
    {{/each}}
</ul>